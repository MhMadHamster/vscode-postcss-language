{
    "name": "postcss-language",
    "displayName": "postcss-sugarss-language",
    "description": "postcss and sugarss syntax support extension for VSCode",
    "version": "1.0.5",
    "publisher": "mhmadhamster",
    "repository": {
        "type": "git",
        "url": "https://github.com/MhMadHamster/vscode-postcss-language"
    },
    "keywords": [
        "postcss",
        "sugarss",
        "cssnext",
        "syntax",
        "grammar",
        "language",
        "vscode"
    ],
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "postcss",
                "aliases": [
                    "PostCSS",
                    "postcss"
                ],
                "extensions": [
                    ".pcss",
                    ".postcss"
                ],
                "configuration": "./language-configuration.json"
            },
            {
                "id": "sugarss",
                "aliases": [
                    "SugarSS",
                    "sugarss"
                ],
                "extensions": [
                    ".sss"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "postcss",
                "scopeName": "source.css.postcss",
                "path": "./syntaxes/postcss.tmLanguage"
            },
            {
                "language": "sugarss",
                "scopeName": "source.css.postcss.sugarss",
                "path": "./syntaxes/sugarss.tmLanguage"
            }
        ]
    },
    "icon": "img/icon.png"
}