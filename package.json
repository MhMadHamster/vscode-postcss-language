{
    "name": "postcss-language",
    "displayName": "PostCSS",
    "description": "postcss syntax support extension for VSCode",
    "version": "1.0.1",
    "publisher": "mhmadhamster",
    "repository": {
        "type": "git",
        "url": "https://github.com/MhMadHamster/vscode-postcss-language"
    },
    "keywords": [
        "postcss",
        "vscode",
        "language",
        "syntax",
        "grammar",
        "cssnext"
    ],
    "engines": {
        "vscode": "^1.10.0"
    },
    "categories": [
        "Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "postcss",
                "aliases": [
                    "PostCSS",
                    "postcss"
                ],
                "extensions": [
                    ".css",
                    ".pcss",
                    ".postcss"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "postcss",
                "scopeName": "scope.css.postcss",
                "path": "./syntaxes/postcss.tmLanguage.json"
            }
        ]
    },
    "icon": "img/icon.png"
}